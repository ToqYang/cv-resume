{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"types": ["node"],
		"baseUrl": ".",
		"paths": {
			"@paths": ["./.svelte-kit/ambient.d.ts"],
			"$lib": ["./src/lib"],
			"$lib/*": ["./src/lib/*"]
		}
	}
} 